<?xml version="1.0"?>
<data>
    <xpath expr="/form/notebook/page/field[@name='date_of_joining']" position="after">
        <label name="gpf_nps"/>
        <field name="gpf_nps"/>
    </xpath>
    <xpath expr="/form/notebook/page[@id='qualifications']" position="after">
        <page id="employee_gpf_details" string="GPF details">
            <group id="gpf_details">
                <label name="gpf_number"/>
                <field name="gpf_number"/>
                <label name="gpf_balance"/>
                <field name="gpf_balance"/>
            </group>
            <newline/>
            <group id="gpf_employee_book">
                <field name="gpf_book"/>
            </group>
            <!-- <field name="gpf_book"/> -->
        </page>
    </xpath>
</data>